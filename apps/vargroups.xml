<bias>

<application id="vargroups" name="VarGroups" info="VarGroup example">
    <description>
		This an example how groups of parameters can be nested.
    </description>
    <author>Bioexploratorium</author>
    <email>pawel@bioexploratorium.pl</email>
    <setup>
      <bin_directory>/var/www/test</bin_directory>
      <help_url>/EP-media/test-help.html</help_url>
	  <param_template>echo &quot;&lt;result&gt;&lt;result&gt;Nothing special.&lt;/result&gt;&lt;/result&gt;&quot;</param_template>
    </setup>
    
    <parameters>
		<email/>
		<section id="SectionInput" title="Groups"/>
		<text id="par1" name="Par 1" tip=""	info="par 1" optional="no" help="#par"/>
		<text id="par2" name="Par 2" tip=""	info="par 2" optional="no" help="#par"/>
		<text id="par3" name="Par 3" tip=""	info="par 3" optional="no" help="#par"/>


		<vargroup id="vargroup1" min="2" max="5" info="vargroup 1" title="VarGroup 1">
			<text id="par1_1" name="Par 1.1" tip=""	info="par 1.1" optional="yes" help="#par"/>
			<text id="par1_2" name="Par 1.2" tip=""	info="par 1.2" optional="yes" help="#par"/>
			<text id="par1_3" name="Par 1.3" tip=""	info="par 1.3" optional="yes" help="#par"/>
		</vargroup>

		<vargroup id="vargroup2" min="2" max="5" info="vargroup 2" title="VarGroup 2">
			<group id="group2_1" grouptype="OR" title="Group 2.1 (OR)" info="group 2.1">
				<text id="par2_1_1" name="Par 2.1.1" tip=""	info="par 2.1.1" optional="yes" help="#par"/>
				<text id="par2_1_2" name="Par 2.1.2" tip=""	info="par 2.1.2" optional="yes" help="#par"/>
				<text id="par2_1_3" name="Par 2.1.3" tip=""	info="par 2.1.3" optional="yes" help="#par"/>
			</group>
		</vargroup>

		<vargroup id="vargroup3" min="2" max="5" info="vargroup 3" title="VarGroup 3">
			<group id="group3_1" grouptype="OR" >
				<text id="par3_1_1" name="Par 3.1.1" tip=""	info="par 3.1.1" optional="yes" help="#par"/>
				<text id="par3_1_2" name="Par 3.1.2" tip=""	info="par 3.1.2" optional="yes" help="#par"/>
				<text id="par3_1_3" name="Par 3.1.3" tip=""	info="par 3.1.3" optional="yes" help="#par"/>
			</group>
		</vargroup>

		<vargroup id="vargroup4" min="2" max="5" info="vargroup 4" title="VarGroup 4">
			<group id="group4_1" grouptype="OR">
				<text id="par4_1_1" name="Par 4.1.1" tip=""	info="par 4.1.1" optional="yes" help="#par"/>
				<text id="par4_1_2" name="Par 4.1.2" tip=""	info="par 4.1.2" optional="yes" help="#par"/>
				<text id="par4_1_3" name="Par 4.1.3" tip=""	info="par 4.1.3" optional="yes" help="#par"/>

				<vargroup id="vargroup4_1_4" min="2" max="5" info="vargroup 4.1.4" title="VarGroup 4.1.4">
					<group id="group4_1_4_1" grouptype="XOR" title="Group 4.1.4.1 (XOR)" info="group 4.1.4.1">
						<text id="par4_1_4_1_1" name="Par 4.1.4.1.1" tip=""	info="par 4.1.4.1.1" optional="yes" help="#par"/>
						<text id="par4_1_4_1_2" name="Par 4.1.4.1.2" tip=""	info="par 4.1.4.1.2" optional="yes" help="#par"/>
						<text id="par4_1_4_1_3" name="Par 4.1.4.1.3" tip=""	info="par 4.1.4.1.3" optional="yes" help="#par"/>
						<group id="group4_1_4_1_1" grouptype="AND" title="Group 4.1.4.1.1 (AND)" optional="yes" info="group 4.1.4.1.1">
							<text id="par4_1_4_1_1_1" name="Par 4.1.4.1.1.1" tip=""	info="par 4.1.4.1.1.1" optional="yes" help="#par"/>
							<text id="par4_1_4_1_1_2" name="Par 4.1.4.1.1.2" tip=""	info="par 4.1.4.1.1.2" optional="yes" help="#par"/>
							<text id="par4_1_4_1_1_3" name="Par 4.1.4.1.1.3" tip=""	info="par 4.1.4.1.1.3" optional="yes" help="#par"/>
						</group>
					</group>
				</vargroup>
				<group id="group4_1_5" grouptype="AND" title="Group 4.1.5 (AND)" info="group 4.1.5">
					<text id="par4_1_5_1" name="Par 4.1.5.1" tip=""	info="par 4.1.5.1" optional="yes" help="#par"/>
					<text id="par4_1_5_2" name="Par 4.1.5.2" tip=""	info="par 4.1.5.2" optional="yes" help="#par"/>
					<text id="par4_1_5_3" name="Par 4.1.5.3" tip=""	info="par 4.1.5.3" optional="yes" help="#par"/>
				</group>
			</group>
		</vargroup>

		<vargroup id="vargroup5" min="2" max="5" info="vargroup 5" title="VarGroup 5">
			<vargroup id="vargroup5_1" min="2" max="5" info="vargroup 5_1" title="VarGroup 5.1">
				<group id="group5_1_1" grouptype="XOR" title="Group 5.1.1 (XOR)" info="group 5.1.1">
					<text id="par5_1_1_1" name="Par 5.1.1.1" tip=""	info="par 5.1.1.1" optional="yes" help="#par"/>
					<text id="par5_1_1_2" name="Par 5.1.1.2" tip=""	info="par 5.1.1.2" optional="yes" help="#par"/>
					<text id="par5_1_1_3" name="Par 5.1.1.3" tip=""	info="par 5.1.1.3" optional="yes" help="#par"/>
					<group id="group5_1_1_4" grouptype="AND" title="Group 5.1.1.4 (AND)" optional="yes" info="group 5.1.1.4">
						<text id="par5_1_1_4_1" name="Par 5.1.1.4.1" tip=""	info="par 5.1.1.4.1" optional="yes" help="#par"/>
						<text id="par5_1_1_4_2" name="Par 5.1.1.4.2" tip=""	info="par 5.1.1.4.2" optional="yes" help="#par"/>
						<text id="par5_1_1_4_3" name="Par 5.1.1.4.3" tip=""	info="par 5.1.1.4.3" optional="yes" help="#par"/>
					</group>
				</group>
			</vargroup>
			<group id="group5_2" grouptype="AND" title="Group 5.2 (AND)" info="group 5.2">
				<text id="par5_2_1" name="Par 5.2.1" tip=""	info="par 5.2.1" optional="yes" help="#par"/>
				<text id="par5_2_2" name="Par 5.2.2" tip=""	info="par 5.2.2" optional="yes" help="#par"/>
				<text id="par5_2_3" name="Par 5.2.3" tip=""	info="par 5.2.3" optional="yes" help="#par"/>
			</group>
		</vargroup>
    </parameters>
</application>

</bias>

